{
  "name": "project-starter",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "dev": "npx nodemon",
    "init:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "init:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert  -d ./src/data-source.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.7.0",
    "node-cache": "^5.1.2",
    "node-telegram-bot-api": "^0.66.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.19",
    "typescript": "^5.3.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.0",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/uuid": "^10.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "ts-node": "10.9.1"
  }
}
